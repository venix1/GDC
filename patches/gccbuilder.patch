 	  CXXFLAGS_FOR_TARGET="-mthreads -O2 -D__USE_MINGW_ACCESS" \
-	  LDFLAGS="-s" BOOT_LDFLAGS="-s" LDFLAGS_FOR_TARGET="-s" \
