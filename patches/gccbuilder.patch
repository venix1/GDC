+	  LDFLAGS="-s" BOOT_LDFLAGS="-s" LDFLAGS_FOR_TARGET="-s" 
 	cd $(GCC_BUILD)/gcc && \
 	 rm -f cc1.exe gnat1.exe cc1plus.exe f951.exe jc1.exe jvgenmain.exe \
 	  cc1obj.exe cc1objplus.exe
@@ -164,8 +163,8 @@ $(GCC_BUILD)/Makefile:
